<?xml version='1.0' encoding='utf-8'?>
<widget id="ultimitats.noorspacejordan" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NoorSpace</name>
    <description>
        NoorSpace - School Mangement Information System
    </description>
    <author email="future.adv2@gmail.com" href="http://www.ultimitats.com">
        ULTIMIT Advanced Turnkey Solutions
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <hook src="scripts/vueify-build.js" type="before_compile" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <engine name="ios" spec="~4.2.1" />
    <engine name="browser" spec="^5.0.3" />
    <engine name="android" spec="^6.2.2" />
</widget>
